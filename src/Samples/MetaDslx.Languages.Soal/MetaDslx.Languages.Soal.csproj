<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\Versions.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <MetaDslxDisableAntlr4RoslynGenerator>false</MetaDslxDisableAntlr4RoslynGenerator>
    <MetaDslxDisableMetaModelGenerator>false</MetaDslxDisableMetaModelGenerator>
  </PropertyGroup>

  <ItemGroup>
    <!--PackageReference Include="Antlr4.CodeGenerator" Version="4.6.6">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference-->
    <PackageReference Include="Antlr4.Runtime" Version="4.6.6" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Main\MetaDslx.CodeAnalysis.Antlr4\MetaDslx.CodeAnalysis.Antlr4.csproj" />
    <ProjectReference Include="..\..\Main\MetaDslx.CodeAnalysis.Common\MetaDslx.CodeAnalysis.Common.csproj" />
    <ProjectReference Include="..\..\Main\MetaDslx.CodeAnalysis.CSharp\MetaDslx.CodeAnalysis.CSharp.csproj" />
    <ProjectReference Include="..\..\Main\MetaDslx.Core\MetaDslx.Core.csproj" />
    <ProjectReference Include="..\..\Main\MetaDslx.Languages.Meta\MetaDslx.Languages.Meta.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MetaDslx.BuildTasks" Version="$(BootstrapVersion)" PrivateAssets="All" />
    <!--PackageReference Include="MetaDslx.CodeAnalysis" Version="$(BootstrapVersion)" /-->
  </ItemGroup>

  <ItemGroup>
    <Antlr4Roslyn Update="Syntax\InternalSyntax\SoalLexer.ag4">
      <OutputLocation>CurrentDirectory</OutputLocation>
    </Antlr4Roslyn>
    <Antlr4Roslyn Update="Syntax\InternalSyntax\SoalParser.ag4">
      <OutputLocation>CurrentDirectory</OutputLocation>
    </Antlr4Roslyn>
  </ItemGroup>

  <ItemGroup>
    <MetaModel Update="Symbols\Soal.mm">
      <OutputLocation>CurrentDirectory</OutputLocation>
    </MetaModel>
  </ItemGroup>

</Project>
